{
  "ip": "127.0.0.1",
  "communicationPort": 9000,
  "asynchronousCommandsPort": 9001,
  "commands": [
    {
      "command": "PRIVMSG (.+) :(.+)",
      "response": "USER $1 0 * :$2",
      "delay" : 2000
    },
    {
      "command": "QUIT (.+)",
      "response": "PRIVMSG $1"
    },
    {
      "command": "QUIT (.+)",
      "response": "QUIT :'jak milo ze napisales'"
    },
    {
      "command": "NICK (.+)",
      "response": "PONG $1"
    }
  ]
}